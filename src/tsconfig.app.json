{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "paths": {
      "~/*": ["./*"],
      "@shared/*": ["./app/shared/*"],
      "@store/*" : ["./app/store/*"],
      "@container/*": ["./app/core/container/*"]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ],
  "types": [
    "gapi",
    "gapi.youtube",
    "gapi.auth2",
    "youtube",
    "jasmine",
    "node"
  ]
}
